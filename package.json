{
  "name": "live",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "next": "^11.1.0",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.4.0",
    "next-optimized-images": "^2.6.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "sass": "^1.26.5"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run sass\" \"npm run local\" ",
    "local": "npm run sass:build && HTTPS=true next",
    "build": "next build",
    "start": "next start",
    "sass": "sass --watch src/assets/scss:src/assets/css --style compressed",
    "sass:build": "rm -rf src/assets/css/app* && sass src/assets/scss:src/assets/css --style compressed"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
